	"scripts": {
		"check-modules": "if [ ! -d 'node_modules/' ]; then npm i; fi",
		"clean-dist": "rm -rf dist/",
		"prewatch": "npm run check-modules",
		"watch": "webpack --watch",
		"prebuild": "npm run check-modules",
		"build": "webpack --color --env.NODE_ENV=production --config webpack.prod.js",
		"prestart": "npm run check-modules",
		"start": "webpack-dev-server --color --config webpack.dev.js",
		"serve": "http-server dist"
	},
