{
	"name": "professional_portfolio",
	"version": "1.0.0",
	"description": "Professional Portfolio",
	"private": true,
	"scripts": {
		"check-modules": "if [ ! -d 'node_modules/' ]; then npm i; fi",
		"clean": "rm -rf publish/ index.html",
		"postinstall": "npm audit fix -f",
		"prestart": "npm run check-modules",
		"start": "./node_modules/.bin/gulp",
		"prebuild": "npm run check-modules && npm run clean",
		"build": "./node_modules/.bin/gulp build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://henriquecarv@github.com/henriquecarv/henriquecarv.github.io.git"
	},
	"keywords": [
		"portfolio",
		"henriquecarv",
		"bootstrap4",
		"jquery",
		"gulp",
		"mdboostrap"
	],
	"author": "Henrique Carvalho da Cruz <henriquecarv.cruz@gmail.com> (https://henriquecarv.com)",
	"license": "MIT",
	"homepage": "https://github.com/henriquecarv/henriquecarv.github.io#readme",
	"issues": "https://github.com/henriquecarv/henriquecarv.github.io/issues",
	"devDependencies": {
		"browser-sync": "^2.26.3",
		"gulp": "^4.0.0",
		"gulp-clean-css": "^3.10.0",
		"gulp-concat-css": "^3.1.0",
		"gulp-htmlmin": "^5.0.1",
		"gulp-sourcemaps": "^2.6.4",
		"gulp-uglify": "^3.0.1"
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^5.4.1",
		"material-design-iconic-font": "^2.2.0",
		"mdbootstrap": "4.5.12"
	}
